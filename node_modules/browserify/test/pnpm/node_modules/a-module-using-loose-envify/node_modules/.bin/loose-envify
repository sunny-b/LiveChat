#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules/.registry.npmjs.org/loose-envify/1.3.1/node_modules/loose-envify/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules/.registry.npmjs.org/loose-envify/1.3.1/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules/.registry.npmjs.org/loose-envify/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/node_modules:/home/goto-bus-stop/Code/browserify/browserify/node_modules:/home/goto-bus-stop/Code/browserify/node_modules:/home/goto-bus-stop/Code/node_modules:/home/goto-bus-stop/node_modules:/home/node_modules:/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules/.registry.npmjs.org/a-module-using-loose-envify/1.0.0/node_modules/loose-envify/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules/.registry.npmjs.org/a-module-using-loose-envify/1.0.0/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules/.registry.npmjs.org/a-module-using-loose-envify/node_modules" "$basedir/node"  "$basedir/../../../loose-envify/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules/.registry.npmjs.org/loose-envify/1.3.1/node_modules/loose-envify/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules/.registry.npmjs.org/loose-envify/1.3.1/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules/.registry.npmjs.org/loose-envify/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/node_modules:/home/goto-bus-stop/Code/browserify/browserify/node_modules:/home/goto-bus-stop/Code/browserify/node_modules:/home/goto-bus-stop/Code/node_modules:/home/goto-bus-stop/node_modules:/home/node_modules:/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules/.registry.npmjs.org/a-module-using-loose-envify/1.0.0/node_modules/loose-envify/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules/.registry.npmjs.org/a-module-using-loose-envify/1.0.0/node_modules:/home/goto-bus-stop/Code/browserify/browserify/test/pnpm/node_modules/.registry.npmjs.org/a-module-using-loose-envify/node_modules" node  "$basedir/../../../loose-envify/cli.js" "$@"
  ret=$?
